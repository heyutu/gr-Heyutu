<?xml version="1.0"?>
<block>
  <name>OFDM_Sync_Receiver</name>
  <key>Heyutu_OFDM_Receiver</key>
  <category>Heyutu</category>
  <import>from Heyutu import Heyutu_OFDM_Receiver</import>
  <make>Heyutu.Heyutu_OFDM_Receiver.ofdm_receiver($fft_length, $cp_length)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>FFT Length</name>
    <key>fft_length</key>
    <type>int</type>
  </param>
  <param>
    <name>CP Length</name>
    <key>cp_length</key>
    <type>int</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>time</name>
    <type>float</type>
  </source>
</block>
